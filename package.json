{
  "name": "facenet",
  "version": "0.0.21",
  "description": "OpenAPI for FaceNet",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "get-pixels": "^3.3.0",
    "ndarray": "^1.0.18",
    "ndarray-distance": "^1.0.0",
    "numjs": "^0.14.0",
    "python-bridge": "^1.0.2"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.30",
    "@types/ndarray": "^1.0.4",
    "@types/node": "^8.0.15",
    "@types/numjs": "^0.14.0",
    "argparse": "^1.0.9",
    "shx": "^0.2.2",
    "tap": "^10.7.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.4.2",
    "typescript": "^2.4.2"
  },
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "lint": "npm run lint:ts && npm run lint:py",
    "lint:ts": "tslint --project tsconfig.json --type-check && npm run clean && tsc --noEmit",
    "lint:py": "pylint src/*.py tests/*.py && npm run lint:flake8 && npm run lint:mypy",
    "lint:flake8": "flake8 src/*.py tesets/*.py",
    "lint:mypy": "mypy --disallow-untyped-defs --python-version=3.6 src/*.py tests/*.py",
    "start": "echo started",
    "test": "npm run test:ts && npm run test:py",
    "test:ts": "npm run lint && tap src/**/*.spec.ts tests/**/*.spec.ts",
    "test:py": "PYTHONPATH='src/:../python-facenet/src/' pytest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zixia/facenet.git"
  },
  "keywords": [
    "facenet",
    "openface"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zixia/facenet/issues"
  },
  "homepage": "https://github.com/zixia/facenet#readme",
  "files": [
    "CHANGELOG",
    "LICENSE",
    "README.md",
    "package.json",
    "dist/bin",
    "dist/index.*",
    "dist/src"
  ]
}
